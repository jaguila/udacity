   "source": [
    "# Requires verification\n",
    "\n",
    "SELECT\n",
    "  ( LocalizedDataStringsProjects.String ) + ' - ' + ( LocalizedDataStringsStudies.String ),\n",
    "  dbo.Sites.Country,\n",
    "  dbo.StudySites.StudySiteNumber,\n",
    "  LocalizedDataStringsSites.String,\n",
    "  dbo.vAccessibleSubjects.SubjectNameForRole,\n",
    "  dbo.fnFormatInstanceName(dbo.Instances.InstanceName,LocalizedDataStringsFolders.String,dbo.Instances.InstanceRepeatNumber,dbo.vAccessibleSubjects.SubjectDate,dbo.Instances.InstanceDate),\n",
    "  dbo.LocalizedDataStrings.String,\n",
    "  dbo.Forms.OID,\n",
    "  dbo.fnFormatDataPageName(dbo.DataPages.DataPageName,dbo.DataPages.PageRepeatNumber,( dbo.LocalizedDataStrings.String ),( ISNULL(LocalizedDataStringsFolders.String,'Root Folder') ),dbo.Instances.InstanceRepeatNumber,dbo.vAccessibleSubjects.SubjectDate,dbo.DataPages.DataPageDate),\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(ObjectStatus_D.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3)) & 256=256 and dbo.fnStatusforRole(COALESCE(ObjectStatus_D.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3)) & 128<>128) THEN 1 ELSE 0 END,\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(ObjectStatus_D.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 256=256 THEN 1 ELSE 0 END ,\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(ObjectStatus_D.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 128=128 THEN 1 ELSE 0 END ,\n",
    "  dbo.Records.RecordPosition,\n",
    "  dbo.Records.RecordID,\n",
    "  LocalizedDataStringsFields.String,\n",
    "  dbo.Fields.SASLabel,\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(ObjectStatus_D.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 2=2 THEN 1 ELSE 0 END ,\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(dbo.ObjectStatusAllRoles.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 128=128 THEN 1 ELSE 0 END ,\n",
    "  CASE WHEN (dbo.fnStatusforRole(COALESCE(dbo.ObjectStatusAllRoles.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 256=256 THEN 1 ELSE 0 END \n",
    "FROM\n",
    "  dbo.CRFVersions INNER JOIN dbo.Forms ON (dbo.CRFVersions.CRFVersionID=dbo.Forms.CRFVersionID)\n",
    "   INNER JOIN dbo.Fields ON (dbo.Fields.FormID=dbo.Forms.FormID)\n",
    "   INNER JOIN dbo.vBODataPoints ON (dbo.Fields.FieldID=dbo.vBODataPoints.FieldID)\n",
    "   INNER JOIN dbo.Records ON (dbo.vBODataPoints.RecordID=dbo.Records.RecordID)\n",
    "   LEFT OUTER JOIN dbo.Instances ON (dbo.Records.InstanceID=dbo.Instances.InstanceID)\n",
    "   LEFT OUTER JOIN dbo.cv_Folders ON (dbo.cv_Folders.FolderID=dbo.Instances.FolderID)\n",
    "   LEFT OUTER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsFolders ON (dbo.cv_Folders.FolderName=LocalizedDataStringsFolders.StringID)\n",
    "   INNER JOIN dbo.DataPages ON (dbo.Records.DataPageID=dbo.DataPages.DataPageID)\n",
    "   INNER JOIN dbo.vAccessibleSubjects ON (dbo.vAccessibleSubjects.SubjectID=dbo.DataPages.SubjectID)\n",
    "   INNER JOIN dbo.StudySites ON (dbo.vAccessibleSubjects.StudySiteID=dbo.StudySites.StudySiteID)\n",
    "   INNER JOIN dbo.Studies ON (dbo.StudySites.StudyID=dbo.Studies.StudyID)\n",
    "   INNER JOIN dbo.Projects ON (dbo.Studies.ProjectID=dbo.Projects.ProjectID)\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsProjects ON ((dbo.Projects.ProjectName=LocalizedDataStringsProjects.StringID  and LocalizedDataStringsProjects.Locale=substring ('eng 55        263       26        5316      ',1,3)))\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsStudies ON ((LocalizedDataStringsStudies.StringID=dbo.Studies.EnvironmentNameID  and LocalizedDataStringsStudies.Locale=substring ('eng 55        263       26        5316      ',1,3)))\n",
    "   INNER JOIN dbo.Sites ON (dbo.StudySites.SiteID=dbo.Sites.SiteID)\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsSites ON ((LocalizedDataStringsSites.StringID=dbo.Sites.SiteNameID AND LocalizedDataStringsSites.Locale=substring ('eng 55        263       26        5316      ',1,3)))\n",
    "   LEFT OUTER JOIN dbo.ObjectStatusAllRoles  ObjectStatus_D ON ((ObjectStatus_D.ObjectID=dbo.DataPages.DataPageID AND ObjectStatus_D.ExpirationDate IS NOT NULL   AND ObjectStatus_D.ObjectTypeID=3))\n",
    "   LEFT OUTER JOIN dbo.ObjectStatusAllRoles ON ((dbo.vBODataPoints.DataPointID=dbo.ObjectStatusAllRoles.ObjectID AND dbo.ObjectStatusAllRoles.ExpirationDate IS NOT NULL   AND dbo.ObjectStatusAllRoles.ObjectTypeID=1))\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsFields ON ((dbo.Fields.FieldName=LocalizedDataStringsFields.StringID  and LocalizedDataStringsFields.Locale=substring ('eng 55        263       26        5316      ',1,3)))\n",
    "   INNER JOIN dbo.LocalizedDataStrings ON ((dbo.LocalizedDataStrings.StringID=dbo.Forms.FormName and dbo.LocalizedDataStrings.Locale=substring ('eng 55        263       26        5316      ',1,3)))\n",
    "  \n",
    "WHERE\n",
    "  ( dbo.Projects.ProjectActive=1  )\n",
    "  AND  ( (dbo.Projects.ProjectID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),1))  )  AND  ( dbo.Studies.StudyActive=1  )\n",
    "  AND  ( (dbo.Studies.StudyID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),2) )  )  AND  ( (dbo.Sites.SiteActive=1 AND dbo.Sites.SiteID IN (SELECT SiteID FROM dbo.cfnRptStudyUserSites(substring('eng 55        263       26        5316      ',5,500))))  )  AND  ( (dbo.StudySites.StudySiteActive=1 AND dbo.StudySites.Deleted=0)  )\n",
    "  AND  ( dbo.StudySites.StudyID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),2)  )  AND  ( dbo.vAccessibleSubjects.SubjectActive=1  )\n",
    "  AND  ( dbo.vAccessibleSubjects.RoleID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3)  )  AND  ( (dbo.cv_Folders.ProjectID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),1) OR dbo.cv_Folders.ProjectID IS NULL)  )  AND  ( ((dbo.Instances.InstanceActive=1 or dbo.Instances.InstanceActive IS NULL) AND (dbo.Instances.Deleted IS NULL OR dbo.Instances.Deleted=0))  )  AND  ( (LocalizedDataStringsFolders.Locale=substring ('eng 55        263       26        5316      ',1,3) OR LocalizedDataStringsFolders.Locale IS NULL)  )  AND  ( (dbo.CRFVersions.ProjectID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),1) AND dbo.CRFVersions.IsDraft=0)  )  AND  ( (dbo.Forms.FormActive=1 AND dbo.Forms.IsDraft=0 AND dbo.Forms.FormID NOT IN (SELECT dbo.FormRestrictViews.FormID from dbo.FormRestrictViews  WHERE dbo.FormRestrictViews.RoleID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3)))  )  AND  ( (dbo.DataPages.DataPageActive=1  AND dbo.DataPages.Deleted =0)  )  AND  ( (dbo.Records.RecordActive=1  AND dbo.Records.Deleted=0)  )  AND  ( (dbo.Fields.FieldActive=1 AND dbo.Fields.IsDraft=0 AND dbo.Fields.FieldID NOT IN (SELECT dbo.FieldRestrictViews.FieldID from dbo.FieldRestrictViews WHERE dbo.FieldRestrictViews.RoleID=dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3)))  )  AND  ( (dbo.vBODataPoints.DataActive=1  AND dbo.vBODataPoints.Deleted=0)  )\n",
    "  AND  ( dbo.vBODataPoints.IsVisible=1  )\n",
    "  AND  \n",
    "  (\n",
    "   CASE WHEN (dbo.fnStatusforRole(COALESCE(dbo.ObjectStatusAllRoles.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 2=2 THEN 1 ELSE 0 END   IN  ( 1  )\n",
    "   AND\n",
    "   CASE WHEN (dbo.fnStatusforRole(COALESCE(dbo.ObjectStatusAllRoles.RoleList,'0'),dbo.cfnBOSecurity(substring('eng 55        263       26        5316      ',5,500),3))) & 128=128 THEN 1 ELSE 0 END   IN  ( 1  )\n",
    "  )\n"
   ]
  }
