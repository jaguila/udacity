  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "Visit name\n",
    "\n",
    "SELECT\n",
    "  LocalizedDataStringsSites.String,\n",
    "  dbo.vAccessibleSubjects.SubjectNameForRole,\n",
    "  dbo.fnFormatInstanceName(dbo.Instances.InstanceName,LocalizedDataStringsFolders.String,dbo.Instances.InstanceRepeatNumber,dbo.vAccessibleSubjects.SubjectDate,dbo.Instances.InstanceDate),\n",
    "  dbo.fnFormatDataPageName(dbo.DataPages.DataPageName,dbo.DataPages.PageRepeatNumber,( dbo.LocalizedDataStrings.String ),( ISNULL(LocalizedDataStringsFolders.String,'Root Folder') ),dbo.Instances.InstanceRepeatNumber,dbo.vAccessibleSubjects.SubjectDate,dbo.DataPages.DataPageDate),\n",
    "  LocalizedDataStringsFields.String,\n",
    "  RTRIM((CASE WHEN (dbo.vBODataPoints.MissingCode IS NOT NULL and dbo.MissingCodes.MissingCodeID=dbo.vBODataPoints.MissingCode) then  \n",
    "(dbo.MissingCodes.MissingCode + ' - ''' + ISNULL(LocalizedDataStringsMC.String,'') + '''') ELSE dbo.vBODataPoints.Data  \n",
    "END) )\n",
    "FROM\n",
    "  dbo.Studies INNER JOIN dbo.StudySites ON (dbo.StudySites.StudyID=dbo.Studies.StudyID)\n",
    "   INNER JOIN dbo.Sites ON (dbo.StudySites.SiteID=dbo.Sites.SiteID)\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsSites ON ((LocalizedDataStringsSites.StringID=dbo.Sites.SiteNameID AND LocalizedDataStringsSites.Locale=substring (@Variable('DBUSER'),1,3)))\n",
    "   INNER JOIN dbo.vAccessibleSubjects ON (dbo.vAccessibleSubjects.StudySiteID=dbo.StudySites.StudySiteID)\n",
    "   INNER JOIN dbo.DataPages ON (dbo.vAccessibleSubjects.SubjectID=dbo.DataPages.SubjectID)\n",
    "   INNER JOIN dbo.Records ON (dbo.Records.DataPageID=dbo.DataPages.DataPageID)\n",
    "   LEFT OUTER JOIN dbo.Instances ON (dbo.Records.InstanceID=dbo.Instances.InstanceID)\n",
    "   LEFT OUTER JOIN dbo.cv_Folders ON (dbo.cv_Folders.FolderID=dbo.Instances.FolderID)\n",
    "   LEFT OUTER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsFolders ON (dbo.cv_Folders.FolderName=LocalizedDataStringsFolders.StringID)\n",
    "   INNER JOIN dbo.vBODataPoints ON (dbo.vBODataPoints.RecordID=dbo.Records.RecordID)\n",
    "   INNER JOIN dbo.Fields ON (dbo.Fields.FieldID=dbo.vBODataPoints.FieldID)\n",
    "   INNER JOIN dbo.Forms ON (dbo.Fields.FormID=dbo.Forms.FormID)\n",
    "   INNER JOIN dbo.CRFVersions ON (dbo.CRFVersions.CRFVersionID=dbo.Forms.CRFVersionID)\n",
    "   INNER JOIN dbo.LocalizedDataStrings ON ((dbo.LocalizedDataStrings.StringID=dbo.Forms.FormName and dbo.LocalizedDataStrings.Locale=substring (@Variable('DBUSER'),1,3)))\n",
    "   INNER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsFields ON ((dbo.Fields.FieldName=LocalizedDataStringsFields.StringID  and LocalizedDataStringsFields.Locale=substring (@Variable('DBUSER'),1,3)))\n",
    "   LEFT OUTER JOIN dbo.MissingCodes ON (dbo.vBODataPoints.MissingCode=dbo.MissingCodes.MissingCodeID)\n",
    "   LEFT OUTER JOIN dbo.LocalizedDataStrings  LocalizedDataStringsMC ON (dbo.MissingCodes.DescriptionID=LocalizedDataStringsMC.StringID)\n",
    "  \n",
    "WHERE\n",
    "( dbo.vBODataPoints.IsHidden=0  )\n",
    "  AND  ( (dbo.Sites.SiteActive=1 AND dbo.Sites.SiteID IN (SELECT SiteID FROM dbo.cfnRptStudyUserSites(substring(@Variable('DBUSER'),5,500))))  )  AND  ( dbo.Studies.StudyActive=1  )\n",
    "  AND  ( (dbo.Studies.StudyID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),2) )  )  AND  ( dbo.vAccessibleSubjects.SubjectActive=1  )\n",
    "  AND  ( dbo.vAccessibleSubjects.RoleID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),3)  )  AND  ( (dbo.cv_Folders.ProjectID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),1) OR dbo.cv_Folders.ProjectID IS NULL)  )  AND  ( ((dbo.Instances.InstanceActive=1 or dbo.Instances.InstanceActive IS NULL) AND (dbo.Instances.Deleted IS NULL OR dbo.Instances.Deleted=0))  )  AND  ( (LocalizedDataStringsFolders.Locale=substring (@Variable('DBUSER'),1,3) OR LocalizedDataStringsFolders.Locale IS NULL)  )  AND  ( (dbo.Forms.FormActive=1 AND dbo.Forms.IsDraft=0 AND dbo.Forms.FormID NOT IN (SELECT dbo.FormRestrictViews.FormID from dbo.FormRestrictViews  WHERE dbo.FormRestrictViews.RoleID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),3)))  )  AND  ( (dbo.DataPages.DataPageActive=1  AND dbo.DataPages.Deleted =0)  )  AND  ( (dbo.CRFVersions.ProjectID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),1) AND dbo.CRFVersions.IsDraft=0)  )  AND  ( (dbo.Fields.FieldActive=1 AND dbo.Fields.IsDraft=0 AND dbo.Fields.FieldID NOT IN (SELECT dbo.FieldRestrictViews.FieldID from dbo.FieldRestrictViews WHERE dbo.FieldRestrictViews.RoleID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),3)))  )  AND  ( (dbo.vBODataPoints.DataActive=1  AND dbo.vBODataPoints.Deleted=0)  )\n",
    "  AND  ( dbo.vBODataPoints.IsVisible=1  )  AND  ( ((dbo.MissingCodes.IsActive=1 OR dbo.MissingCodes.IsActive IS NULL) AND  (dbo.MissingCodes.Deleted=0 OR dbo.MissingCodes.Deleted IS NULL))  )  AND  ( (LocalizedDataStringsMC.Locale=substring (@Variable('DBUSER'),1,3) OR LocalizedDataStringsMC.Locale IS NULL)  )  AND  ( (dbo.StudySites.StudySiteActive=1 AND dbo.StudySites.Deleted=0)  )\n",
    "  AND  ( dbo.StudySites.StudyID=dbo.cfnBOSecurity(substring(@Variable('DBUSER'),5,500),2)  )  AND  ( (dbo.Records.RecordActive=1  AND dbo.Records.Deleted=0)  )\n",
    "  AND  \n",
    "  (\n",
    "   LocalizedDataStringsFields.String  IN  ( N'SVSTDAT'  )\n",
    "   AND\n",
    "   dbo.LocalizedDataStrings.String  IN  ( N'Date of Visit'  )\n",
    "  )\n"
   ]
  },
